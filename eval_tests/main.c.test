	printf(" return: %d\n",    printf("|%%%s|", "hello") - 2);
	printf(" return: %d\n", ft_printf("|%%%s|", "hello") - 2);
	// printf(" return: %d\n", printf("|%-*s|", 10, "hello") - 2);
	// printf(" return: %d\n", printf("|%-*.*s|", 10, 4, "hello") - 2);
	#ifdef DEBUG
